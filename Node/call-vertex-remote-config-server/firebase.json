{
  "functions": [
    {
      "source": "functions",
      "codebase": "call-vertex-remote-config-server",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local",
        "*.bak"
      ]
    }
  ],
  "emulators": {
    "functions": {
      "port": 5001
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  },
  "hosting": {
    "public": "client/dist",
    "ignore": ["firebase.json", "**/node_modules/**"]
  }
}
