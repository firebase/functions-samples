{
  "rules": {
    "users": {
      ".read": true,
      "$uid": {
        ".write": "auth.uid === $uid"
      }
    },
    "followers": {
      "$followedUid": {
        "$followerUid": {
          ".read": "auth.uid === $followerUid",
          ".write": "auth.uid === $followerUid"
        }
      }
    }
  }
}
